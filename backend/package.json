{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch-build\" \"npm run watch-dev\"",
    "watch-build": "swc src --watch --out-dir build",
    "watch-dev": "nodemon --watch \"build\" -e js build/bin/www.js",
    "build": "swc src --out-dir build",
    "build:clean": "npm run clean && npm run build",
    "start": "node build/bin/www.js",
    "start:build": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts --fix",
    "prepare": "node .husky/install.mjs"
  },
